{
  "name": "hotmeteor/eco-cli",
  "description": "Eco CLI for Laravel .env syncing",
  "type": "project",
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@hotmeteor.com"
    }
  ],
  "bin": [
    "eco"
  ],
  "require": {
    "php": "^7.4",
    "illuminate/container": "^6.0|^7.0|^8.0",
    "illuminate/filesystem": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "knplabs/github-api": "^2.15",
    "php-http/guzzle6-adapter": "^1.1",
    "ramsey/uuid": "^3.8|^4.0",
    "symfony/console": "^4.2|^5.0",
    "symfony/process": "^4.2|^5.0",
    "symfony/var-dumper": "^4.2|^5.0",
    "symfony/yaml": "^4.2|^5.0",
    "vlucas/phpdotenv": "^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Eco\\EcoCli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eco\\EcoCli\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
